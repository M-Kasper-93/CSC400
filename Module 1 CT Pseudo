//add items
//remove items
//check for items
//count occurrence of item

START
  CREATE array of length = 5

//to add item:
  ADD item
  UPDATE item counter

//to remove item:
   FOR i from 0 TO index -1
      IF item(s) exists THEN
        REMOVE item
      END IF
    END FOR

//to check for item:
    FOR i from 0 TO index -1
      IF item(s) exists THEN
        RETURN true
      END IF
    END FOR

//to count occurences of item:
    FOR i from 0 TO index -1
      IF item(s) exists THEN
        COUNT item ocuurences
        RETURN count
      END IF
    END FOR

//to print

  FOR i from 0 TO index -1
    PRINT items
  END FOR
END




